class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        curr = nums[0]
        maxv = nums[0]
        s = 0
        for e in range(1, len(nums)):
            if curr < 0:
                s = e 
                curr = nums[e]
            else:
                curr += nums[e]
            maxv = max(maxv, curr) 
        
        return maxv
